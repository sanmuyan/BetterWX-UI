{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-set-title",
    "opener:default",
    "dialog:default",
    "store:default",
    "process:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://gitee.com/*"
        }
      ]
    }   
  ]
}