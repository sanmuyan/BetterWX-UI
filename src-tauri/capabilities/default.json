{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "process:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://*"},{ "url": "http://*"}]
    }
  ]
}