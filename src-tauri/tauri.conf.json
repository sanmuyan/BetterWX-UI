{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "betterwx-ui",
  "version": "1.1.1",
  "identifier": "com.betterwx-ui.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:23323",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "betterwx-ui 1.1.1",
        "width": 640,
        "minWidth": 640,
        "height": 360,
        "minHeight": 360
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "nsis",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  }
}